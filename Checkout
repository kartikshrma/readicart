                                          }
                                        } else {
                                          // rotateButton
                                          if (animationsMap[
                                                  'iconButtonOnActionTriggerAnimation'] !=
                                              null) {
                                            animationsMap[
                                                    'iconButtonOnActionTriggerAnimation']!
                                                .controller
                                                .reverse();
                                          }
                                        }
                                      },
                                    ).animateOnActionTrigger(
                                      animationsMap[
                                          'iconButtonOnActionTriggerAnimation']!,
                                    ),
                                  ],
